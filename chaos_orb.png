    (function () {
        function getMaxHeight($elm){
            return Math.max.apply(Math, $.makeArray($elm).map(el => $(el).outerHeight()));
        }

        function getRows(elm) {
            var objectRows = $.makeArray(elm).reduce(function (acc, e) {
                var _top = $(e).offset().top;
                if (!acc[_top]) {
                    acc[_top] = [];
                }
                acc[$(e).offset().top].push(e);
                return acc;
            }, {});

            return Object.keys(objectRows).map(function (key) {
                return objectRows[key];
            });
        }
        function _async(arrFn, onEndCb) {
            if(arrFn.length === 0){
                onEndCb();
            }

            var state = {
                count: arrFn.length,
                onEnd: function onEnd() {
                    this.count--;
                },
                isAllDone: function isAllDone() {
                   return this.count === 0;
                }
            };

            arrFn.map(function (fn) {
                return fn(function () {
                    state.onEnd();
                    if (state.isAllDone()) {
                        onEndCb();
                    }
                });
            });
        };
        function fixMinHeight(scope){
            getRows($('.ajax_block_product', scope ? scope instanceof jQuery.Event ? document : scope : document)).map(function (row) {
                var $row = $(row);
                // $row.find('.product-container').css('min-height','330px');
